{
  "parser": "babel-eslint",
  "rules": {
    "no-undef": 0,
    "no-underscore-dangle": 0,
    "curly": [2, "multi-line"],
    "space-infix-ops": 0,
    "quotes": [1, "single", "avoid-escape"],
    "new-cap": 0,
    "no-shadow": 0
  },
  "plugins": [
  ],
  "ecmaFeatures": {
    "modules" : true
  }
}
